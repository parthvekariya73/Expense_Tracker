<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         version="6.0"
         xmlns="https://jakarta.ee/xml/ns/jakartaee"
         xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>userRegister</servlet-name>
        <servlet-class>com.servlets.userRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userRegister</servlet-name>
        <url-pattern>/userRegister</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SaveExpenseServlet</servlet-name>
        <servlet-class>com.servlets.SaveExpenseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveExpenseServlet</servlet-name>
        <url-pattern>/SaveExpenseServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdateExpenseServlet</servlet-name>
        <servlet-class>com.servlets.UpdateExpenseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateExpenseServlet</servlet-name>
        <url-pattern>/UpdateExpenseServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DeleteExpenseServlet</servlet-name>
        <servlet-class>com.servlets.DeleteExpenseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DeleteExpenseServlet</servlet-name>
        <url-pattern>/DeleteExpenseServlet</url-pattern>
    </servlet-mapping>

</web-app>
